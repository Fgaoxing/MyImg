{
    "version": 2,
  "github": {
    "silent": true
  },
  "builds": [
              {
            "src": "package.json",
            "use": "vercel-sapper"
        },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
